{
  "name": "votingportal-app-tests",
  "version": "2.0.0",
  "description": "In this repo will be developed cypress tests for Voting Portal.",
  "main": "index.js",
  "devDependencies": {
    "cucumber-html-reporter": "5.5.0",
    "cypress": "^9.7.0",
    "cypress-cucumber-preprocessor": "^4.3.0",
    "cypress-grep": "^2.5.3",
    "cypress-sql-server": "^1.0.0",
    "del": "^6.0.0",
    "eslint": "^7.27.0",
    "eslint-plugin-cypress": "^2.11.3",
    "lodash": "^4.17.21",
    "node-xlsx": "^0.17.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "test-results/cucumber/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:qa": "cypress run --env configFile=qa",
    "lint": "eslint cypress",
    "precy:run": "rmdir /s /q test-results",
    "cy:run:smoke": "cypress run --env grepTags=@smoke"
  },
  "repository": {
    "type": "git",
    "url": "https://glasslewis@dev.azure.com/glasslewis/Development/_git/VotingPortal-app-tests"
  },
  "author": "",
  "license": "ISC",
  "keywords": []
}
